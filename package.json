{
  "name": "comic-commander",
  "version": "0.0.6",
  "license": "MIT",
  "private": false,
  "scripts": {
    "npm:publish": "npm publish",
    "run:api": "node apps/api/main.js",
    "run:comiccommander": "node apps/comiccommander/main.js",

    "docker:print-container-id:api": "docker ps | grep  ' comic-commander_api' | awk '{ print $1}'",
    "docker:print-container-id:web": "docker ps | grep  ' comic-commander_web' | awk '{ print $1}'",
    "docker:logs:api": "docker logs $(docker ps | grep  ' comic-commander_api' | awk '{ print $1 }') ",
    "docker:logs:web": "docker logs $(docker ps | grep  ' comic-commander_web' | awk '{ print $1 }') ",
    "docker:logs-f:api": "docker logs $(docker ps | grep  ' comic-commander_api' | awk '{ print $1 }') -f",
    "docker:logs-f:web": "docker logs $(docker ps | grep  ' comic-commander_web' | awk '{ print $1 }') -f",

    "docker:container-ls": "docker container ls",
    "docker:container-remove": "docker image rm comic-commander_api -f && docker image rm comic-commander_web -f",

    "docker:images": "docker images",
    "docker:image-remove": "docker image rm comic-commander_api -f && docker image rm comic-commander_web -f",

    "docker:compose:build": "docker compose build",
    "docker:compose:up": "docker compose up",
    "docker:compose:up-and-build": "docker compose up --build"
  },
  "dependencies": {
    "@angular/animations": "14.1.2",
    "@angular/cdk": "14.1.2",
    "@angular/common": "14.1.2",
    "@angular/compiler": "14.1.2",
    "@angular/core": "14.1.2",
    "@angular/forms": "14.1.2",
    "@angular/material": "14.1.2",
    "@angular/platform-browser": "14.1.2",
    "@angular/platform-browser-dynamic": "14.1.2",
    "@angular/router": "14.1.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nrwl/angular": "14.5.6",
    "cache-manager": "^4.1.0",
    "fs-extra": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.2",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "14.1.2",
    "@angular/language-service": "14.1.2",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.5.6",
    "@nrwl/eslint-plugin-nx": "14.5.6",
    "@nrwl/jest": "14.5.6",
    "@nrwl/linter": "14.5.6",
    "@nrwl/nest": "14.5.6",
    "@nrwl/node": "14.5.6",
    "@nrwl/nx-cloud": "14.4.0",
    "@nrwl/workspace": "14.5.6",
    "@types/cache-manager": "^4.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.4.1",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.5.1",
